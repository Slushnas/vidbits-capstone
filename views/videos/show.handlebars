<div class="contents-container">
  <div class="video-card">
    <div class="video-title">
      <h1>{{title}}</h1>
    </div>
    <iframe class="video-player"
    src="{{url}}">
    </iframe>
    <div class="video-description">
      <p>{{description}}</p>
    </div>
  </div>
  <div id="video-options">
    <form action="/videos/{{_id}}/edit" method="get">
      <input id="edit-button" class="input-button" type="submit" value="Edit"/>
    </form>
    <form action="/videos/{{_id}}/deletions" method="post">
      <input id="delete-button" class="input-button" type="submit" value="Delete"/>
    </form>
  </div>
</div>
